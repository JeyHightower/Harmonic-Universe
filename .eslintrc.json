{
  "root": true,
  "env": {
    "browser": true,
    "es2020": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "process": true,
    "global": true,
    "window": true,
    "setTimeout": true,
    "clearTimeout": true,
    "setInterval": true,
    "clearInterval": true,
    "WebSocket": true,
    "Event": true,
    "CustomEvent": true,
    "URLSearchParams": true,
    "URL": true,
    "FormData": true,
    "FileReader": true,
    "Blob": true,
    "AbortController": true,
    "requestAnimationFrame": true,
    "cancelAnimationFrame": true,
    "alert": true,
    "TextEncoder": true,
    "self": true,
    "globalThis": true,
    "document": true,
    "ReadableStream": true,
    "Request": true,
    "Response": true,
    "btoa": true,
    "XMLHttpRequest": true,
    "HTMLElement": true,
    "SVGElement": true,
    "Element": true,
    "Node": true,
    "Document": true,
    "WorkerGlobalScope": true,
    "Buffer": true,
    "MessageChannel": true,
    "performance": true,
    "getComputedStyle": true,
    "queueMicrotask": true,
    "setImmediate": true,
    "require": true,
    "__dirname": true,
    "module": true,
    "exports": true,
    "define": true,
    "EVALEX": true,
    "EVALEX_TRUSTED": true,
    "CONSOLE_MODE": true,
    "SECRET": true,
    "dirname": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "plugin:react-hooks/recommended",
    "plugin:import/recommended"
  ],
  "ignorePatterns": [
    "dist",
    "node_modules",
    "build",
    "*.d.ts",
    "backend/static",
    "venv",
    "venv/**",
    "frontend/dist",
    "frontend/dist/**",
    "frontend/node_modules",
    "frontend/build",
    "frontend/scripts",
    "frontend/scripts/**",
    "frontend/src/__tests__/**",
    "frontend/src/assets/**",
    "scripts",
    "scripts/**",
    "**/vendor-*",
    "**/*.config.js",
    "**/__tests__/**",
    "**/test_*.js",
    "*.min.js",
    "*.worker.js",
    "frontend/public/**",
    "**/*.production.min.js",
    "**/static/**",
    "**/ant-icons/**",
    "frontend/update-paths.js",
    "vite.config.js",
    "index.js",
    "test_*.js",
    "**/site-packages/**",
    "**/services/**",
    "**/utils/**",
    "**/hooks/**",
    "**/debug/**",
    "**/error/**",
    "**/modals/**",
    "**/icons/**",
    "**/components/**/*.js",
    "**/features/**/*.js",
    "**/features/**/*.jsx",
    "**/components/**/*.jsx",
    "frontend/src/client.js",
    "frontend/public/**/*.js",
    "**/lib/python*/**",
    "**/contrib/**/*.js",
    "**/werkzeug/**/*.js",
    "**/emscripten/**/*.js",
    "**/update-paths.js",
    "build.test/**",
    "**/*.be1add8b.js",
    "**/*.fa18e013.js",
    "**/*.f9606c35.js",
    "**/*.5d603e0b.js",
    "**/*.8edb30e2.js",
    "**/*.ec1cb8a7.js",
    "**/*.4fc76094.js",
    "**/*.cd93f9c5.js",
    "**/*.a508a592.js",
    "**/*.c46623e8.js",
    "**/*.442e5e08.js",
    "**/*.6485732f.js",
    "**/*.07f7a30a.js",
    "**/*.70450c41.js",
    "**/*.e46571dd.js",
    "**/*.e3ba29e5.js",
    "**/*.f5f62bcb.js",
    "**/*.d3b49c6d.js",
    "**/*.e7130502.js",
    "**/*.66bccfad.js",
    "**/*.617853c7.js",
    "**/*.5f12d50d.js",
    "**/*.f8e7421b.js",
    "**/*.d96f7130.js",
    "**/*.dcafff4c.js",
    "**/*.ee856eec.js",
    "**/*.e47bcfeb.js",
    "**/*.cded68ae.js",
    "**/*.5e297c7e.js",
    "**/*.27f9a3b5.js",
    "**/*.e7f533f2.js",
    "**/*.c96386b3.js",
    "**/*.0b4b1ccd.js",
    "**/*.1ece7284.js",
    "**/*.07e4d08d.js",
    "**/*.398f9a2a.js",
    "**/*.8730038f.js",
    "**/*.60b42083.js",
    "**/*.fcf170e6.js",
    "**/*.399edcf9.js",
    "**/*.3612091e.js",
    "**/*.31b1c8da.js",
    "**/*.087493a9.js",
    "**/*.c58b7b1a.js",
    "**/*.fbc2a4f7.js",
    "**/*.47edd63d.js"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx"]
      },
      "alias": {
        "map": [
          ["@", "./frontend/src"],
          ["frontend", "./frontend"],
          ["backend", "./backend"]
        ],
        "extensions": [".js", ".jsx"]
      }
    }
  },
  "plugins": [
    "react",
    "react-hooks",
    "react-refresh",
    "import"
  ],
  "rules": {
    "react/prop-types": "off",
    "react/display-name": "off",
    "react/no-unescaped-entities": "off",
    "react/jsx-key": "error",
    "react-hooks/exhaustive-deps": "off",
    "react-hooks/rules-of-hooks": "off",
    "no-unused-vars": ["off", {
      "varsIgnorePattern": "React|^_",
      "argsIgnorePattern": "^_"
    }],
    "react-refresh/only-export-components": "off",
    "import/no-unresolved": "off",
    "import/named": "off",
    "import/default": "off",
    "import/namespace": "off",
    "import/order": "off",
    "no-empty": "off",
    "no-useless-escape": "off",
    "no-cond-assign": "off",
    "no-fallthrough": "off",
    "no-func-assign": "off",
    "no-prototype-builtins": "off",
    "no-control-regex": "off",
    "no-misleading-character-class": "off",
    "no-undef": "off",
    "no-useless-catch": "off",
    "no-case-declarations": "off",
    "no-constant-binary-expression": "off"
  }
} 