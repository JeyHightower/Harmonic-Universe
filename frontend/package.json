{
  "name": "frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --force --clearScreen=false --host --cors",
    "dev:debug": "VITE_DEBUG=true vite --force --clearScreen=false --host --cors",
    "build": "NODE_ENV=production CI=false VITE_APP_ENV=production vite build --emptyOutDir",
    "postbuild": "mkdir -p ../backend/static && cp -r dist/* ../backend/static/ || echo 'Note: Could not copy to backend/static'",
    "preview": "vite preview",
    "start": "vite --port 5174",
    "serve": "vite preview --port 5174",
    "lint": "eslint --ignore-pattern 'src/assets/*' --ignore-pattern 'src/__tests__/*' --ignore-pattern 'public/*' --ignore-pattern 'scripts/*' --ignore-pattern '**/*.min.js' --ignore-pattern 'babel.config.js' --ignore-pattern 'src/components/common/ExportModal.jsx' --ignore-pattern '**/*-*.js' .",
    "lint:fix": "eslint --ignore-pattern 'src/assets/*' --ignore-pattern 'src/__tests__/*' --ignore-pattern 'public/*' --ignore-pattern 'scripts/*' --ignore-pattern '**/*.min.js' --ignore-pattern 'babel.config.js' --ignore-pattern 'src/components/common/ExportModal.jsx' --ignore-pattern '**/*-*.js' . --fix"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.15",
    "@mui/material": "^5.14.15",
    "@mui/x-date-pickers": "^7.28.3",
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^4.24.12",
    "axios": "^1.6.2",
    "framer-motion": "^12.6.5",
    "history": "^5.3.0",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0",
    "redux-persist": "^6.0.0",
    "three": "^0.157.0",
    "tone": "^14.7.77"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/runtime": "^7.24.0",
    "@emotion/babel-plugin": "^11.11.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.5.3",
    "terser": "^5.39.0",
    "vite": "^4.5.12"
  },
  "description": "Root package for Harmonic Universe - A web application that allows users to create and explore musical universes based on physics principles.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JeyHightower/Harmonic-Universe.git"
  },
  "author": "",
  "license": "MIT",
