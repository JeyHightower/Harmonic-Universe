# Harmonic Universe Frontend Environment Variables
# This file should be copied to .env for local development

# API Configuration
VITE_API_URL=http://localhost:5001/api
VITE_API_BASE_URL=http://localhost:5001

# Application Configuration
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development
VITE_PORT=5173
VITE_HOST=localhost

# Feature Flags
VITE_ENABLE_MFA=false
VITE_ENABLE_PASSWORD_RESET=true
VITE_ENABLE_ACCOUNT_LOCKOUT=true
VITE_ENABLE_DEBUG_LOGGING=true
VITE_MOCK_AUTH_IN_DEV=false
VITE_FEATURE_DEMO_MODE=false
VITE_FEATURE_OFFLINE_MODE=true
VITE_FEATURE_DEBUG_MODE=true
VITE_FEATURE_ANALYTICS=false
VITE_FEATURE_PERFORMANCE_MONITORING=false
VITE_MOCK_API=false

# Debug Settings
VITE_DEBUG=true

# Asset Configuration
VITE_CDN_URL=
VITE_PUBLIC_URL=/
VITE_BASE_URL=/ 